{
  "docu": [
    "https://stylelint.io/user-guide/rules",
    "https://github.com/stylelint-stylistic/stylelint-config/blob/main/.stylelintrc.json"],
  "plugins": ["stylelint-order", "@stylistic/stylelint-plugin", "stylelint-scss"],
  "customSyntax": "postcss-scss",
  "extends": ["stylelint-config-recommended-scss", "@stylistic/stylelint-config"],
  "rules": {
    "custom-property-pattern": ["^[a-z][a-z0-9]*(-[a-z0-9]+)*$|^[a-z][a-z0-9]*(-[a-z0-9]+)$", {
      "message": "Expected custom property name to be in camelCase or kebab-case"
    }],
    "declaration-property-value-allowed-list": [{
      "/^font-size$/": ["/^var\\(--font-size-[a-z-]+\\)$/"],
      "/^font-weight$/": ["/^var\\(--font-weight-[a-z-]+\\)$/"],
      "/^z-index$/": ["/^var\\(--z-level-[0-9]+\\)$/"]
    }, {
      "message": "Expected value to be a variable like var(--*)"
      }
    ],
    "selector-pseudo-class-no-unknown": [true, {
      "ignorePseudoClasses": ["global", "local"]
    }],
    "rule-empty-line-before": ["always", {
      "except": ["after-single-line-comment", "first-nested"]
    }],
    "selector-class-pattern": ["^[a-z][a-zA-Z0-9]*$", {
      "message": "Selector should be in camelCase"
    }],
    "scss/percent-placeholder-pattern": ["^[a-z][a-zA-Z0-9]*$", {
      "message": "Placeholder selector should be in camelCase"
    }],
    "selector-type-case": ["lower", {
      "ignoreTypes": ["/^\\$\\w+/"]
    }],
    "value-keyword-case": ["lower", {
      "ignoreKeywords": ["dummyValue"]
    }],
    "block-no-empty": true,
    "no-irregular-whitespace": true,
    "length-zero-no-unit": true,
    "shorthand-property-no-redundant-values": true,
    "selector-pseudo-element-colon-notation": "single",
    "alpha-value-notation": "number",
    "color-function-notation": "legacy",
    "declaration-property-value-no-unknown": true,
    "no-descending-specificity": true,
    "declaration-block-no-redundant-longhand-properties": true,
    "no-duplicate-selectors": null,
    "at-rule-descriptor-no-unknown": true,
    "at-rule-no-deprecated": true,
    "at-rule-prelude-no-invalid": true,
    "declaration-property-value-keyword-no-deprecated": true,
    "no-unknown-custom-media": true,
    "media-query-no-invalid": true,
    "media-feature-name-value-no-unknown": true,
    "selector-anb-no-unmatchable": true,
    "scss/at-rule-no-unknown": true,
    "@stylistic/indentation": 2,
    "@stylistic/number-leading-zero": "never",
    "@stylistic/string-quotes": "double",
    "@stylistic/declaration-block-semicolon-newline-after": "always",
    "@stylistic/block-opening-brace-newline-after": "always",
    "@stylistic/block-closing-brace-newline-after": ["always", {
      "ignoreAtRules": ["if", "else"]
    }],
    "order/order": [
      "at-rules",
      "dollar-variables",
      "declarations",
      "custom-properties"
    ],
